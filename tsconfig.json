{
    "include": ["src"],
    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "bundler",
        "lib": ["ES2022"],
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "noEmit": true,
        "skipLibCheck": true,
        "isolatedModules": true
    }
}
